{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "svgtex",
  "version": "1.1.0",
  "identifier": "com.svgtex.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "svgtex",
        "width": 600,
        "height": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
